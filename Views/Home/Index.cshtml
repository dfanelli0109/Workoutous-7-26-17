@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html ng-app="WorkOutous">
<head>
    <title>WorkOutous</title>
    <meta charset="utf-8">
    <!--Responsive Meta Tag-->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/workoutous.css" media="screen" and min-width="801px" />
    <link rel="stylesheet" href="~/css/site1.css" media="handheld" and max-width="800px" />
    
 
    
    <base href="/" />
    
    <!--Materialize CSS-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.99.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Workoutous CSS-->
   
</head>
<body ng-controller="Main as vm">
    <!--NAV-->
    <div id="nav">
        <nav>
            <div class="nav nav-wrapper" id="wrappers">
                <a ui-sref="home" class="brand-logo center" id="navbrand">Workoutous</a>
                <a href="#" data-activates="mobile-demo" class="button-collapse" id="icon"><i class="large material-icons">menu</i></a>
                <ul class="right hide-on-med-and-down">
                 
                    <li><a class="tab" id="navlink" ui-sref="login">Log In</a></li>
                    <li><a class="tab" id="navlink" ui-sref="register">Register</a></li>
                    <li><a class="tab" id="navlink" ui-sref="front">Continue as Visitor</a></li>
                </ul>
                <ul class="side-nav" id="mobile-demo">
                   _
                    <a ui-sref="front"> <img class="logo_on_login" id="logologin" src="~/images/Workoutlogo8.jpg"> </a>
                    <form>
                        <div class="row" id="loginmenu">
                            <!--username-->
                                <label id="un" for="username">Username</label>
                                <input id="username" type="text" class="validate">
                                
                           
                            <!--password-->
                            <label id="pw" for="password">Password</label>
                                <input id="password" type="password" class="validate">
                               
                           
                        
                    </form>
                    <li><a class="tab" id="navlink2" ui-sref="front">Log In</a></li>
                    <li><a class="tab" id="navlink2" ui-sref="register">Register</a></li>
                    <li><a class="tab" id="navlink2" ui-sref="front">Let's get Started</a></li>
                    </ul>
            </div>
        </nav>
    </div>
        
        <ui-view></ui-view>
    </div>

    <!--Button to scroll to top of page-->
    <button class="" onclick="topFunction()" id="topBtn" title="Go to top"> 🢁 TOP </button>

    <!--FOOTER-->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <div id="copy"> &copy; 2017 Workoutous</div>
            
                </div>
            </div>
        </div>
    </footer>

    <!---Lib Files-->
    <script src="~/lib/angular/angular.js"></script>
    <script src="~/lib/angular-resource/angular-resource.js"></script>
    <script src="~/lib/angular-ui-router/release/angular-ui-router.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    
    <!---Controllers & Services-->
    <script src="~/ngApp/controllers/MainController.js"></script>
    <script src="~/ngApp/services/MainService.js"></script>
    <script src="~/ngApp/controllers/LogInController.js"></script>

    <!--angular app-->
    <script src="~/ngApp/app.js"></script>
    <!--Workoutous JS file-->
    <script src="js/workoutous.js"></script>
    <script src="jquery-3.2.1.min.js"></script>
    <!--Materialize JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.99.0/js/materialize.min.js"></script>
</body>
</html>
